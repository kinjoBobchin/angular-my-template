{
  "name": "angular-my-template",
  "version": "0.1.6",
  "scripts": {
    "ng": "ng",
    "serve": "ng serve",
    "start": "node server",
    "build": "ng build",
    "build:prod": "ng build --aot --prod",
    "lint": "ng lint",
    "test": "cypress run --spec 'src/app/**/*.spec.ts'",
    "test:e2e": "./node_modules/.bin/cypress open",
    "cypress:install": "cypress install",
    "test:wait": "wait-on http-get://localhost:4200 && yarn test",
    "postinstall": "node ./ng-add-pug-loader.js && ng build --aot --prod",
    "deploy:build": "ng build --prod --base-href 'https://kinjobobchin.github.io/angular-my-template/'",
    "deploy": "angular-cli-ghpages --dir='dist/angular-my-template'",
    "update": "yarn run ng update @angular/cli; and yarn run ng update --all",
    "direnvify": "node direnvify.ts"
  },
  "engines": {
    "node": "<= 10.x"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.1.0",
    "@angular/cdk": "^7.1.0",
    "@angular/cli": "^7.0.6",
    "@angular/common": "^7.1.0",
    "@angular/compiler": "^7.1.0",
    "@angular/compiler-cli": "^7.1.0",
    "@angular/core": "^7.1.0",
    "@angular/flex-layout": "^7.0.0-beta.19",
    "@angular/forms": "^7.1.0",
    "@angular/http": "^7.1.0",
    "@angular/material": "^7.1.0",
    "@angular/platform-browser": "^7.1.0",
    "@angular/platform-browser-dynamic": "^7.1.0",
    "@angular/router": "^7.1.0",
    "@types/ramda": "^0.26.0",
    "compression": "^1.7.3",
    "core-js": "^2.5.4",
    "express": "^4.16.3",
    "hammerjs": "^2.0.8",
    "ng-cli-pug-loader": "^0.1.3",
    "node-sass": "^4.9.3",
    "ramda": "^0.26.0",
    "rxjs": "^6.3.3",
    "typescript": "3.1.6",
    "wait-on": "^3.2.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.6",
    "@angular/language-service": "^7.1.0",
    "@bahmutov/add-typescript-to-cypress": "^2.0.0",
    "@types/jasmine": "~3.3.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^10.12.10",
    "angular-cli-ghpages": "^0.5.3",
    "apply-loader": "^2.0.0",
    "codelyzer": "^4.5.0",
    "cypress": "^3.1.2",
    "pug": "^2.0.3",
    "pug-loader": "^2.4.0",
    "puppeteer": "^1.8.0",
    "ts-node": "~7.0.1",
    "tslint": "~5.11.0"
  }
}
